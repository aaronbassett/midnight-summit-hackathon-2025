# Makefile for generating Rust bindings from {{contract_name}} contract
# Generated by {{TEMPLATE_NAME}} v{{TEMPLATE_VERSION}} on {{CURRENT_DATE}}

generate:
	forge bind --crate-name {{bindings_crate_name}} --bindings-path ./bindings --alloy-version 1.0.36 --force --no-metadata --overwrite --skip script

check:
	forge bind --crate-name {{bindings_crate_name}} --bindings-path ./bindings --alloy-version 1.0.36 --force --no-metadata --skip script

.PHONY: generate
.PHONY: check
